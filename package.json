    "@types/jest": "^26.0.19",
    "@types/react-redux": "^7.1.16",
    "eslint": "^7.20.0",
    "eslint-import-resolver-typescript": "^2.4.0",
    "husky": "4.3.5",
    "prettier": "^2.1.2",
    "source-map-explorer": "^2.5.2",
    "ts-node": "^9.1.1"
  }
