{
  "name": "galaxy-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "start": "react-scripts start",
    "build": "CI=false react-scripts build",
    "build-win": "set CI=false&&react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint:js": "eslint --ext \".tsx\" --ignore-path .gitignore . --fix",
    "lint-win": "npm run lint:js",
    "lint": "eslint 'src/**/*.{js,jsx,ts,tsx}'",
    "format:check": "prettier --check --loglevel error 'src/**/*.{js,jsx,ts,tsx}'",
    "format:write": "prettier --write 'src/**/*.{js,jsx,ts,tsx}'",
    "generate:config": "ts-node --compiler-options '{\"module\":\"commonjs\"}' scripts/generateConfig"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn format:write",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@binance-chain/bsc-connector": "^1.0.0",
    "@crowdin/crowdin-api-client": "^1.10.4",
    "@ethersproject/abi": "^5.0.7",
    "@material-ui/core": "^4.12.3",
    "@pancakeswap-libs/sdk": "^1.0.1",
    "@pancakeswap-libs/uikit": "^0.14.1",
    "@popperjs/core": "^2.10.1",
    "@reach/dialog": "^0.16.0",
    "@reach/portal": "^0.16.0",
    "@reduxjs/toolkit": "^1.5.0",
    "@types/react": "^17.0.2",
    "@types/react-dom": "^17.0.1",
    "@types/react-router-dom": "^5.1.5",
    "@types/styled-components": "^5.1.7",
    "@uniswap/token-lists": "^1.0.0-beta.25",
    "@uniswap/v2-periphery": "^1.1.0-beta.0",
    "@web3-react/abstract-connector": "^6.0.7",
    "@web3-react/core": "^6.1.9",
    "@web3-react/injected-connector": "^6.0.7",
    "@web3-react/walletconnect-connector": "^6.2.4",
    "@web3-react/walletlink-connector": "^6.2.5",
    "bignumber.js": "^9.0.0",
    "canvas-confetti": "^1.3.3",
    "chart.js": "^2.9.4",
    "date-fns": "^2.17.0",
    "ethers": "^5.0.31",
    "glx-uikit": "^0.16.1",
    "graphql": "^15.5.0",
    "graphql-request": "^3.4.0",
    "i18next": "^20.6.1",
    "i18next-browser-languagedetector": "^6.1.2",
    "i18next-xhr-backend": "^3.2.2",
    "js-cookie": "^2.2.1",
    "lodash": "^4.17.20",
    "lodash.flatmap": "^4.5.0",
    "multicodec": "^3.2.1",
    "multihashes": "^4.0.3",
    "node-fetch": "^2.6.1",
    "polished": "^4.1.3",
    "react": "^17.0.1",
    "react-chartjs-2": "^2.11.1",
    "react-countup": "^4.3.3",
    "react-device-detect": "^1.17.0",
    "react-dom": "^17.0.1",
    "react-feather": "^2.0.8",
    "react-i18next": "^11.12.0",
    "react-popper": "^2.2.5",
    "react-redux": "^7.2.2",
    "react-router-dom": "^5.2.0",
